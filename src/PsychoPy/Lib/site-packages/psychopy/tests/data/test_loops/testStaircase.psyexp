      <Param name="max value" updates="None" val="1" valType="num"/>
      <Param name="min value" updates="None" val="0" valType="num"/>
      <Param name="nReps" updates="None" val="5" valType="num"/>
      <Param name="name" updates="None" val="trials" valType="code"/>
      <Param name="start value" updates="None" val="0.5" valType="num"/>
      <Param name="step sizes" updates="None" val="[0.8,0.8,0.4,0.4,0.2]" valType="list"/>
