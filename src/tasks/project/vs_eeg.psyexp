        <Param val="experiment_stage = &quot;practice&quot;;&amp;#10;practice_answer = null;&amp;#10;group_id = practice_order_table[Number.parseInt((practice_idx / 4))];&amp;#10;show_id = (show_orders[group_id][((practice_idx + 1) % 4)] - 1);&amp;#10;circles = [circle_1, circle_2, circle_3, circle_4];&amp;#10;rects = [rect_6, rect_9, rect_10, rect_13];&amp;#10;for (var i, _pj_c = 0, _pj_a = util.range(0, 4), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    i = _pj_a[_pj_c];&amp;#10;    if ((i === show_id)) {&amp;#10;        circles[i].opacity = 1;&amp;#10;        rects[i].opacity = 0;&amp;#10;    } else {&amp;#10;        circles[i].opacity = 0;&amp;#10;        rects[i].opacity = 1;&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="&amp;#10;# CONDITIONAL FLAG&amp;#10;experiment_stage = &quot;practice&quot;&amp;#10;practice_answer = None&amp;#10;    &amp;#10;# UPDATE TEXT&amp;#10;group_id = practice_order_table[(int)(practice_idx/4)]&amp;#10;show_id = show_orders[group_id][(practice_idx+1)%4] - 1 &amp;#10;&amp;#10;&amp;#10;circles = [circle_1,circle_2,circle_3,circle_4]&amp;#10;rects = [rect_6,rect_9,rect_10,rect_13]&amp;#10;&amp;#10;for i in range(0,4):&amp;#10;    if i==show_id:&amp;#10;        circles[i].opacity = 1&amp;#10;        rects[i].opacity = 0&amp;#10;    else:&amp;#10;        circles[i].opacity = 0&amp;#10;        rects[i].opacity = 1&amp;#10;        &amp;#10;        " valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
        <Param val="&amp;#10;if(practice_answer==None and practice_response.status==STARTED and len(practice_response.keys)&gt;0):&amp;#10;    practice_answer = practice_response.keys[0]&amp;#10;&amp;#10;    &amp;#10;# bnu&amp;#10;if practice_cross.status == STARTED:&amp;#10;    if tThisFlipGlobal &gt; practice_cross.tStartRefresh + 1-frameTolerance:&amp;#10;        for c in goodportary:#bnu&amp;#10;            try:&amp;#10;                c.send(mark_weizhi[show_id])&amp;#10;            except:&amp;#10;                tkinter.messagebox.showinfo('错误','脑电采集设备连接断开')&amp;#10;                core.quit()" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="if ((((practice_answer === null) &amp;&amp; (practice_response.status === PsychoJS.Status.STARTED)) &amp;&amp; (practice_response.keys.length &gt; 0))) {&amp;#10;    practice_answer = practice_response.keys[0];&amp;#10;}&amp;#10;if ((practice_cross.status === PsychoJS.Status.STARTED)) {&amp;#10;    if ((tThisFlipGlobal &gt; ((practice_cross.tStartRefresh + 1) - frameTolerance))) {&amp;#10;        for (var c, _pj_c = 0, _pj_a = goodportary, _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;            c = _pj_a[_pj_c];&amp;#10;            try {&amp;#10;                c.send(mark_weizhi[show_id]);&amp;#10;            } catch(e) {&amp;#10;                tkinter.messagebox.showinfo(&quot;\u9519\u8bef&quot;, &quot;\u8111\u7535\u91c7\u96c6\u8bbe\u5907\u8fde\u63a5\u65ad\u5f00&quot;);&amp;#10;                core.quit();&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
