        <Param val="&amp;#10;# CONDITIONAL FLAG&amp;#10;experiment_stage = &quot;practice&quot;&amp;#10;practice_pressed = False&amp;#10;practice_answer = None&amp;#10;    &amp;#10;# UPDATE TEXT&amp;#10;current_num = shownum[experiment_idx]&amp;#10;current_mirror = is_mirror[experiment_idx]&amp;#10;&amp;#10;practice_image.image = image_root+&quot;/&quot;+get_img_name(current_level,current_num,current_mirror)+&quot;.jpg&quot;" valType="extendedCode" updates="constant" name="Begin Routine"/>
