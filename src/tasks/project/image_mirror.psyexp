    <Routine name="init">
      <CodeComponent name="variable_init">
        <Param val="&amp;#10;#创建变量时不需要$，引用时才需要&amp;#10;&amp;#10;    &amp;#10;level_total = 6&amp;#10;current_level = 1&amp;#10;level_images = 6&amp;#10;degree_divider = 360 / level_images&amp;#10;tip_duration = 10&amp;#10;image_duration = 5&amp;#10;&amp;#10;image_root = &quot;./assets/image_mirror&quot;&amp;#10;def get_img_name(level,idx,is_mirrored):# idx从0开始&amp;#10;    name = str(level).zfill(2)+&quot;-&quot;+str(int(idx*degree_divider))&amp;#10;    if(is_mirrored):&amp;#10;        name += &quot;-m&quot;&amp;#10;    return name&amp;#10;    &amp;#10;# TEXT CONFIG&amp;#10;guide_text = &quot;您好，欢迎参加本次实验\n每段实验开始时，屏幕左侧呈现原图、右侧呈现镜像\n随后会呈现数张旋转后的图片，请做如下反应：\n若由原图旋转所得，请按“F”\n若由镜像旋转所得，请按“J”\n明白后按空格进入练习&quot;&amp;#10;practice_confirm_text = &quot;练习已结束\n若您已了解本实验操作请按“Y”键，进入正式实验；\n若您还未熟悉实验操作请按“N”键，重新进行练习。&quot;&amp;#10;experiment_rest_text = &quot;下一阶段难度将会提升\n请休息一下，当您觉得状态恢复良好时\n可随时按空格键继续。&quot;&amp;#10;experiment_end_text = &quot;试验结束，感谢参与\n按空格退出&quot;&amp;#10;&amp;#10;# CONDITIONAL FLAG&amp;#10;experiment_stage = &quot;before_experiment&quot;&amp;#10;&amp;#10;# PRACTICE CONFIG&amp;#10;practice_repeats = 4&amp;#10;&amp;#10;# EXPERIMENT CONFIG&amp;#10;experiment_block_nums = level_total&amp;#10;experiment_block_trials = 12" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="level_total = 6;&amp;#10;current_level = 1;&amp;#10;level_images = 6;&amp;#10;degree_divider = (360 / level_images);&amp;#10;tip_duration = 10;&amp;#10;image_duration = 5;&amp;#10;image_root = &quot;./assets/image_mirror&quot;;&amp;#10;function get_img_name(level, idx, is_mirrored) {&amp;#10;    var name;&amp;#10;    name = ((level.toString().zfill(2) + &quot;-&quot;) + Number.parseInt((idx * degree_divider)).toString());&amp;#10;    if (is_mirrored) {&amp;#10;        name += &quot;-m&quot;;&amp;#10;    }&amp;#10;    return name;&amp;#10;}&amp;#10;guide_text = &quot;\u60a8\u597d\uff0c\u6b22\u8fce\u53c2\u52a0\u672c\u6b21\u5b9e\u9a8c\n\u6bcf\u6bb5\u5b9e\u9a8c\u5f00\u59cb\u65f6\uff0c\u5c4f\u5e55\u5de6\u4fa7\u5448\u73b0\u539f\u56fe\u3001\u53f3\u4fa7\u5448\u73b0\u955c\u50cf\n\u968f\u540e\u4f1a\u5448\u73b0\u6570\u5f20\u65cb\u8f6c\u540e\u7684\u56fe\u7247\uff0c\u8bf7\u505a\u5982\u4e0b\u53cd\u5e94\uff1a\n\u82e5\u7531\u539f\u56fe\u65cb\u8f6c\u6240\u5f97\uff0c\u8bf7\u6309\u201cF\u201d\n\u82e5\u7531\u955c\u50cf\u65cb\u8f6c\u6240\u5f97\uff0c\u8bf7\u6309\u201cJ\u201d\n\u660e\u767d\u540e\u6309\u7a7a\u683c\u8fdb\u5165\u7ec3\u4e60&quot;;&amp;#10;practice_confirm_text = &quot;\u7ec3\u4e60\u5df2\u7ed3\u675f\n\u82e5\u60a8\u5df2\u4e86\u89e3\u672c\u5b9e\u9a8c\u64cd\u4f5c\u8bf7\u6309\u201cY\u201d\u952e\uff0c\u8fdb\u5165\u6b63\u5f0f\u5b9e\u9a8c\uff1b\n\u82e5\u60a8\u8fd8\u672a\u719f\u6089\u5b9e\u9a8c\u64cd\u4f5c\u8bf7\u6309\u201cN\u201d\u952e\uff0c\u91cd\u65b0\u8fdb\u884c\u7ec3\u4e60\u3002&quot;;&amp;#10;experiment_rest_text = &quot;\u4e0b\u4e00\u9636\u6bb5\u96be\u5ea6\u5c06\u4f1a\u63d0\u5347\n\u8bf7\u4f11\u606f\u4e00\u4e0b\uff0c\u5f53\u60a8\u89c9\u5f97\u72b6\u6001\u6062\u590d\u826f\u597d\u65f6\n\u53ef\u968f\u65f6\u6309\u7a7a\u683c\u952e\u7ee7\u7eed\u3002&quot;;&amp;#10;experiment_end_text = &quot;\u8bd5\u9a8c\u7ed3\u675f\uff0c\u611f\u8c22\u53c2\u4e0e\n\u6309\u7a7a\u683c\u9000\u51fa&quot;;&amp;#10;experiment_stage = &quot;before_experiment&quot;;&amp;#10;practice_repeats = 4;&amp;#10;experiment_block_nums = level_total;&amp;#10;experiment_block_trials = 12;&amp;#10;" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="&amp;#10;&amp;#10;&amp;#10;# CONDITIONAL FLAG&amp;#10;experiment_stage = &quot;begin_experiment&quot;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="experiment_stage = &quot;begin_experiment&quot;;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
